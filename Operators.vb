Module Operators

    Sub Main()
        Console.WriteLine(2 + 2)
        Console.WriteLine(40 + 40)
        Console.WriteLine(27 / 3)   'Regular division
        Console.WriteLine(1 Mod 2)  'Integer division
        Console.WriteLine(1 \ 2)    'Remainder only divison
        Console.ReadLine()
    End Sub

End Module
